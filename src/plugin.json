{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Explore Traces",
  "id": "grafana-exploretraces-app",
  "autoEnabled": true,
  "info": {
    "keywords": ["app", "tempo", "traces", "explore"],
    "description": "Query-less exploration of trace data stored in Tempo",
    "author": {
      "name": "Grafana"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "Explore",
      "path": "/a/%PLUGIN_ID%/",
      "action": "datasources:explore",
      "addToNav": true,
      "defaultNav": true
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=11.2.0",
    "plugins": []
  }
}
